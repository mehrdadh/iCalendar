{
    "manifest_version": 3,
    "name": "iCalendar",
    "version": "1.0.0",
    "description": "Import iCalendar (.ics) files to Google Calendar with simple drag and drop.",
    "action": {
        "default_popup": "popup.html",
        "default_title": "iCalendar"
    },
    "permissions": [
        "identity",
        "storage"
    ],
    "host_permissions": [
        "https://www.googleapis.com/*",
        "https://accounts.google.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
